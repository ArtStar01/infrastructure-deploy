{
  "ecmaFeatures": {},
  "env": {
    "browser": true,
    "node": true,
    "amd": false,
    "mocha": false,
    "jasmine": true
  },

  "plugins": [
    "angular"
  ],

  "globals": {
    "angular": true,
    "$": true,
    "_": true,
    "lodash": true
  },

  "rules": {
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-comma-dangle": 0,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-continue": 0,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 0,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-label": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "all"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 1,
    "no-lone-blocks": 2,
    "no-lonely-if": 0,
    "no-loop-func": 2,
    "no-mixed-requires": [0, false],
    "no-mixed-spaces-and-tabs": [2, false],
    "linebreak-style": [0, "unix"],
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 0,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-modules": 0,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-throw-literal": 0,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-use-before-define": 0,
    "no-void": 2,
    "no-var": 0,
    "prefer-const": 0,
    "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": 2,

    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "brace-style": [0, "1tbs"],
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "complexity": [1, 7],
    "computed-property-spacing": [0, "never"],
    "consistent-return": 2,
    "consistent-this": [0, "that"],
    "curly": [2, "all"],
    "default-case": 0,
    "dot-location": 0,
    "dot-notation": [2, { "allowKeywords": true, "allowPattern": "delete" }],
    "eol-last": 0,
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": [0, "expression"],
    "generator-star": 0,
    "generator-star-spacing": 0,
    "strict": 0,
    "quotes": [2, "single"],
    "quote-props": [2, "as-needed", {"keywords": true}],
    "guard-for-in": 0,
    "handle-callback-err": 0,
    "indent": 0,
    "key-spacing": [0, { "beforeColon": true, "afterColon": true, "align": "value" }],
    "lines-around-comment": 0,
    "max-depth": [2, 3],
    "max-len": [0, 80, 4],
    "max-nested-callbacks": [1, 3],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": [0, "always"],
    "object-curly-spacing": [2, "always"],
    "object-shorthand": [0, "always"],
    "one-var": [0, "always"],
    "operator-assignment": [2, "always"],
    "operator-linebreak": [0, "after"],
    "padded-blocks": [2, "never"],
    "radix": 2,
    "semi": 2,
    "semi-spacing": [2, {"before": false, "after": true}],
    "sort-vars": 0,
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "spaced-comment": 0,
    "spaced-line-comment": [0, "always"],
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "wrap-regex": 0,
    "yoda": [2, "never"],

    "angular/angularelement": 0, // 2 precise it
    "angular/controller-as": 0,
    "angular/controller-as-route": 0,
    "angular/controller-as-vm": [0, "self"],
    "angular/controller-name": [2, "/[A-Z].*Ctrl$/"],
    "angular/definedundefined": 2,
    "angular/di": [0, "function"],
    "angular/directive-name": 0,
    "angular/document-service": 2,
    "angular/empty-controller": 2,
    "angular/foreach": 0,
    "angular/filter-name": 0,
    "angular/function-type": [0, "anonymous"],
    "angular/interval-service": 2,
    "angular/json-functions": 2,
    "angular/module-getter": 0,
    "angular/module-name": 0,
    "angular/module-setter": 2,
    "angular/no-digest": 2,   // precise it
    "angular/no-jquery-angularelement": 2,
    "angular/no-private-call": 0,
    "angular/no-services": [0, ["$http", "$resource", "Restangular"]],
    "angular/no-service-method": 0,
    "angular/on-watch": 0,
    "angular/service-name": [0, "/.*(Factory|Srv)$/"],
    "angular/timeout-service": 2,
    "angular/typecheck-array": 2,
    "angular/typecheck-date": 2,
    "angular/typecheck-function": 2,
    "angular/typecheck-number": 2,
    "angular/typecheck-object": 2,
    "angular/typecheck-regexp": 2,
    "angular/typecheck-string": 2,
    "angular/window-service": 2
  }
}
